//clone()
import java.util.*;
class copying {
    static void copy(int arr1[],int n){
        int arr2[]=arr1.clone();
        for(int i=0;i<n;i++){
            System.out.print(arr1[i]+" ");
            System.out.print(arr2[i]+" ");
        }
    }
    public static void main(String[] args) {
        Scanner sc = new Scanner(System.in);
        int n = sc.nextInt();
        int arr1[] = new int[n];
        for(int i=0;i<n;i++){
            arr1[i]=sc.nextInt();
        }
        copy(arr1,n);
    }
}
//copyOf()
import java.util.*;
class copying {
    static void copy(Integer arr1[],int n){
        Integer arr2[]=Arrays.copyOf(arr1,n);
        for(int i=0;i<n;i++){
            System.out.print(arr1[i]+" ");
            System.out.print(arr2[i]+" ");
        }
    }
    public static void main(String[] args) {
        Scanner sc = new Scanner(System.in);
        int n = sc.nextInt();
        Integer arr1[] = new Integer[n];
        for(int i=0;i<n;i++){
            arr1[i]=sc.nextInt();
        }
        copy(arr1,n);
    }
}
